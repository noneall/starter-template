{
  "name": "@unfolio/ui-react",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "tags": [
      "ui",
      "lib",
      "react"
    ],
    "targets": {
      "storybook": {
        "options": {
          "cwd": "packages/ui-react",
          "command": "storybook dev",
          "port": 4402
        },
        "executor": "nx:run-commands",
        "configurations": {},
        "parallelism": true
      }
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ]
}
